{
  "name": "Smart iFrame",
  "author": {
    "name": "Hubbub Studios",
    "email": "taylor@hubbub.dev",
    "url": "https://github.com/hubbubdev/zendesk-easy-iframe"
  },
  "defaultLocale": "en",
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/app.html",
        "flexible": true
      }
    }
  },
  "version": "1.0.0",
  "frameworkVersion": "2.0",
  "parameters": [
    {
      "name": "url",
      "type": "text",
      "default": "https://google.com?q=${ticketId}",
      "required": true
    },
    {
      "name": "height",
      "type": "number",
      "default": 300
    }
  ]
}
